@model IEnumerable<JSONEmpresa.Models.Funcionario>

@{
    ViewBag.Title = "PegarDados";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>PegarDados</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.salario)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.salario)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
    }

</table>

<script>
    $(function () {
        $.ajax({
            dataType: "json",
            type: "GET",
            url: "/Home/GetDados",
            success: function (dados) {
                $(dados).each(function (i) {
                    document.writeln("<p>Nome: " + dados[i].Nome + "</p>" , "<p>Idade: " + dados[i].URL + "</p>")
                });
            }
        });
    });
</script>


